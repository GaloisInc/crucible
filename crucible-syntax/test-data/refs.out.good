(defun @refs ((s String)) (Ref String)
   (start st:
      (let x (ref s))
      (let y (deref x))
      (set-ref! x (string-append y s))
      (drop-ref! x)
      (return x)))

%0
  % 3:5
  $1 = newref($0)
  % 4:5
  $2 = !$1
  % 5:17
  $3 = appendString($2, $0)
  % 5:5
  $1 := $3
  % 6:5
  drop $1
  % 7:5
  return $1
